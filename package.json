{
    "name": "conacoin",
    "version": "0.0.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/vxggangxv/conacoin.io.git"
    },
    "scripts": {
        "start": "pm2 start ecosystem.config.js --env production",
        "reload": "pm2 reload conacoin",
        "restart": "pm2 restart conacoin",
        "stop": "pm2 stop conacoin",
        "delete": "pm2 delete conacoin",
        "startdev": "pm2 start ecosystem.config.js",
        "build": "cross-env NODE_ENV=production nodemon -e scss,js,html server.js",
        "test": "cross-env NODE_ENV=test nodemon -e scss,js,html server.js",
        "dev": "cross-env NODE_ENV=development nodemon -e scss,js,html server.js",
        "ui": "browser-sync start --proxy=localhost:8000 --files='**/*.css, **/*.html, **/*.js' --ignore=node_modules --reload-delay 10 --no-ui --no-notify",
        "lint": "eslint --ext .js controllers"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "cheerio-httpcli": "^0.7.4",
        "connect-flash": "^0.1.1",
        "connect-session-sequelize": "^6.0.0",
        "cookie-parser": "^1.4.5",
        "cross-env": "^7.0.0",
        "csurf": "^1.10.0",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "ejs": "~2.6.1",
        "express": "~4.16.1",
        "express-paginate": "^1.0.0",
        "express-session": "^1.17.0",
        "http-errors": "~1.6.3",
        "iamporter": "^1.4.2",
        "moment": "^2.24.0",
        "morgan": "~1.9.1",
        "multer": "^1.4.2",
        "mysql2": "^2.0.2",
        "node-sass-middleware": "0.11.0",
        "node-schedule": "^1.3.2",
        "nodemailer": "^6.4.2",
        "nunjucks": "^3.2.0",
        "passport": "^0.4.1",
        "passport-facebook": "^3.0.0",
        "passport-local": "^1.0.0",
        "request": "^2.88.2",
        "request-promise": "^4.2.5",
        "sequelize": "4.42.0",
        "socket.io": "^2.3.0",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "browser-sync": "^2.26.7",
        "connect-browser-sync": "^2.1.0",
        "eslint": "^6.8.0",
        "prettier": "^1.19.1",
        "reload": "^3.0.4"
    }
}
